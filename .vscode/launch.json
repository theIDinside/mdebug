    "version": "0.2.0",
    "configurations": [
        {
            "type": "midas-rr",
            "request": "attach",
            "name": "Launch replay session",
            "use-dap": true,
            "trace": "Off",
            "cwd": "${workspaceFolder}",
            "setupCommands": [
                "set sysroot /",
                "set debuginfod enabled off",
                "set auto-load safe-path /"
            ]
        }
    ]
}